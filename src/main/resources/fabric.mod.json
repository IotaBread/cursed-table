{
  "schemaVersion": 1,
  "id": "cursed_table",
  "version": "${version}",

  "name": "Cursed table",
  "description": "Tired of having to deal with villagers only to get a mending book for 64 emeralds? This mod solves it! You can find any enchantment on the table now that wasn't available before... including curses",
  "authors": [
    "ByMartrixx"
  ],
  "contact": {
    "sources": "https://github.com/ByMartrixx/cursed-table",
    "issues": "https://github.com/ByMartrixx/cursed-table/issues"
  },

  "license": "MIT",
  "icon": "assets/cursed_table/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.bymartrixx.cursed.table.CursedTable"
    ]
  },
  "mixins": [
    "cursed_table.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "minecraft": "1.16.x"
  }
}
